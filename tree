#include <cstdio>
#include <vector>

using namespace std;

vector<vector<int> > tree(51);

int cnt;

void order(int root, int d){
	if(tree[root].size()==0){
		cnt++;
	}
	for(int i=0;i<tree[root].size();i++){
		if(d!=tree[root][i]){
			order(tree[root][i], d);
		}
		if(tree[root].size()==1 && d==tree[root][i]){
			cnt++;
		}
	}
}

int main(){
	int n, root;
	scanf("%d", &n);
	for(int i=0;i<n;i++){
		int x;
		scanf("%d",&x);
		if(x==-1) root=i;
		else tree[x].push_back(i);
	}
	int d;
	scanf("%d",&d);

	if(d!=root){
		order(root, d);
		printf("%d\n",cnt);
	}
	else{
		printf("0\n");
	}

	return 0;
}
